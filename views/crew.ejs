        <script>

            
            var el = [...document.getElementsByClassName("card")];
            el.forEach(async function (e) {
                const xhttp = new XMLHttpRequest();
                xhttp.onload = function() {
                    let user = JSON.parse(this.responseText);

                    e.childNodes[1].childNodes[1].childNodes[1].src=`https://cdn.discordapp.com/avatars/${e.dataset.id}/${user.avatar}.${user.avatar.startsWith("a_") ? "gif" : "jpg"}?size=1024`
                    e.childNodes[1].childNodes[3].childNodes[1].textContent=user.username
                    e.childNodes[1].childNodes[3].childNodes[3].textContent="Md."
                }
                xhttp.open("GET", `https://userfetchapi.eryip78.repl.co/getuser?id=${e.dataset.id}`);
                xhttp.send();
            });
